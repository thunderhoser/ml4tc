#!/bin/tcsh

#SBATCH --job-name="match_cnn_predictions_to_ships"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="gpuwf"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
#SBATCH --time=12:00:00
#SBATCH --array=1-3
#SBATCH --exclude=h28n12
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=match_cnn_predictions_to_ships_%A_%a.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tc_standalone/ml4tc"
set INPUT_SHIPS_PREDICTION_FILE_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tc_project/oper_stext/ships_ri_predictions_2010-2014_2020-2021.nc"
set TOP_MODEL_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tc_models/ri_paper_2023_experiment/multimodel_ensembles"

set CIRA_IR_LAG_TIME_COUNTS=(   "0" "1" "1")
set USE_SHIPS_ENVIRO_FLAGS=(    "1" "0" "1")
set USE_SHIPS_HISTORICAL_FLAGS=("1" "0" "1")
set USE_SHIPS_SATELLITE_FLAGS=( "1" "0" "1")
set USE_TEMPORAL_DIFF_FLAGS=(   "0" "0" "0")
set DROPOUT_RATES=(             "0.8" "0.7" "0.8")

set num_cira_ir_lag_times=${CIRA_IR_LAG_TIME_COUNTS[$SLURM_ARRAY_TASK_ID]}
set use_ships_enviro=${USE_SHIPS_ENVIRO_FLAGS[$SLURM_ARRAY_TASK_ID]}
set use_ships_historical=${USE_SHIPS_HISTORICAL_FLAGS[$SLURM_ARRAY_TASK_ID]}
set use_ships_satellite=${USE_SHIPS_SATELLITE_FLAGS[$SLURM_ARRAY_TASK_ID]}
set use_temporal_diffs=${USE_TEMPORAL_DIFF_FLAGS[$SLURM_ARRAY_TASK_ID]}
set dropout_rate=${DROPOUT_RATES[$SLURM_ARRAY_TASK_ID]}

set model_dir_name="${TOP_MODEL_DIR_NAME}/num-cira-ir-lag-times=${num_cira_ir_lag_times}_use-ships-enviro=${use_ships_enviro}_use-ships-historical=${use_ships_historical}_use-ships-satellite=${use_ships_satellite}_use-temporal-diffs=${use_temporal_diffs}_dense-dropout-rate=${dropout_rate}/full_ensemble"

python3 -u "${CODE_DIR_NAME}/match_cnn_predictions_to_ships.py" \
--input_cnn_prediction_file_name="${model_dir_name}/real_time_testing/isotonic_regression/predictions.nc" \
--input_ships_prediction_file_name="${INPUT_SHIPS_PREDICTION_FILE_NAME}" \
--use_ri_consensus=0 \
--use_ri_dtops=0 \
--use_td_to_ts_lge=0 \
--output_cnn_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/cnn_predictions_cf_basic.nc" \
--output_ships_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/ships_predictions_basic.nc"

python3 -u "${CODE_DIR_NAME}/match_cnn_predictions_to_ships.py" \
--input_cnn_prediction_file_name="${model_dir_name}/real_time_testing/isotonic_regression/predictions.nc" \
--input_ships_prediction_file_name="${INPUT_SHIPS_PREDICTION_FILE_NAME}" \
--use_ri_consensus=0 \
--use_ri_dtops=1 \
--use_td_to_ts_lge=0 \
--output_cnn_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/cnn_predictions_cf_dtops.nc" \
--output_ships_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/ships_predictions_dtops.nc"

python3 -u "${CODE_DIR_NAME}/match_cnn_predictions_to_ships.py" \
--input_cnn_prediction_file_name="${model_dir_name}/real_time_testing/isotonic_regression/predictions.nc" \
--input_ships_prediction_file_name="${INPUT_SHIPS_PREDICTION_FILE_NAME}" \
--use_ri_consensus=1 \
--use_ri_dtops=0 \
--use_td_to_ts_lge=0 \
--output_cnn_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/cnn_predictions_cf_consensus.nc" \
--output_ships_prediction_file_name="${model_dir_name}/real_time_testing_matched_with_ships/isotonic_regression/ships_predictions_consensus.nc"
